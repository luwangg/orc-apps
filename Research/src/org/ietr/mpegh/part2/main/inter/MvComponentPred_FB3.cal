	guard
		iLoop < numTiles
	do
		tilesBeg[iLoop] := [tilesCoord[0]/4, tilesCoord[1]/4];
		tilesEnd[iLoop] := [tilesCoord[2]/4, tilesCoord[3]/4];
		iLoop := iLoop + 1;
	end

	getTilesCoord.done: action ==>
	guard
		iLoop = numTiles
	end

